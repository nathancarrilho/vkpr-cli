{
  "inputs": [
    {
      "tutorial": "Specifies the domain to use in the application",
      "label": "Type the Keycloak domain:",
      "name": "domain",
      "type": "text",
      "default": "localhost"
    },
    {
      "tutorial": "Specifies if the application will have HTTPS",
      "label": "Secure ?",
      "name": "secure",
      "type": "bool",
      "default": "false",
      "items": [
        "true",
        "false"
      ]
    },
    {
      "tutorial": "Specifies the Super Admin username",
      "label": "Type the Admin username:",
      "name": "admin_user",
      "type": "text",
      "default": "admin"
    },
    {
      "tutorial": "Specifies the Super Admin password",
      "label": "Type the Admin password:",
      "name": "admin_password",
      "type": "password",
      "default": "vkpr123",
      "pattern": {
        "regex": "^([A-Za-z0-9-]{7,})$",
        "mismatchText": "Password too weak"
      }
    },
    {
      "tutorial": "Specifies if the application will have High Availability",
      "label": "HA ?",
      "name": "HA",
      "type": "bool",
      "default": "false",
      "items": [
        "true",
        "false"
      ]
    },
    {
      "tutorial": "Specifies if output the values from the application",
      "label": "Dry-run ?",
      "name": "dry_run",
      "type": "bool",
      "default": "false",
      "items": [
        "true",
        "false"
      ]
    },
    {
      "tutorial": "Specifies the Postgresql password",
      "name": "password",
      "type": "CREDENTIAL_POSTGRES_PASSWORD"
    }
  ],
  "template": "shell-bat",
  "templateRelease:": "2.16.2"
}