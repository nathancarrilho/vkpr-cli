{
  "inputs": [
    {
      "tutorial": "Specifies the domain to use in the application",
      "label": "Type the stack domain:",
      "name": "domain",
      "type": "text",
      "default": "localhost"
    },
    {
      "tutorial": "Specifies if the application will have HTTPS",
      "label": "Secure ?",
      "name": "secure",
      "type": "bool",
      "default": "false",
      "items": [
        "true",
        "false"
      ]
    },
    {
      "tutorial": "Specifies the Super Admin password",
      "label": "Type the Grafana Admin password:",
      "name": "grafana_password",
      "type": "text",
      "default": "vkpr123",
      "pattern": {
        "regex": "^([A-Za-z0-9-]{7,})$",
        "mismatchText": "Password too weak"
      }
    },
    {
      "tutorial": "Enable Alert-manager to be installed",
      "label": "Install Alert-Manager ?",
      "name": "alertmanager",
      "type": "bool",
      "default": "false",
      "items": [
        "true",
        "false"
      ]
    },
    {
      "tutorial": "Specifies if the application will have High Availability",
      "condition": {
        "variable": "alertmanager",
        "operator": "==",
        "value": "true"
      },
      "label": "HA ?",
      "name": "HA",
      "type": "bool",
      "default": "false",
      "items": [
        "true",
        "false"
      ]
    },
    {
      "tutorial": "Specifies if output the values from the application",
      "label": "Dry-run ?",
      "name": "dry_run",
      "type": "bool",
      "default": "false",
      "items": [
        "true",
        "false"
      ]
    }
  ],
  "template": "shell-bat",
  "templateRelease:": "2.16.2"
}